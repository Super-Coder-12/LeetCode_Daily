class Solution:
    def productExceptSelf(self, nums: List[int]) -> List[int]:
        n = len(nums)
        res = [1] * n ###### 初始为1

        for i in range(1,n):
            res[i] = res[i-1] * nums[i-1] # prefix

        postfix = nums[-1] ################ n-2
        for i in range(n-2,-1,-1):
            res[i] *= postfix
            postfix *= nums[i]
        return res 
        
