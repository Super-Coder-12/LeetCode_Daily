class Solution:
    def longestConsecutive(self, nums: List[int]) -> int:
       # if not nums:
        #    return 0
        
        st = set(nums)
        res = 0
        curNum = 0
        l = 1 ##### 新开一个
        for num in st:
            if num - 1 not in st:
                l = 1
                curNum = num 
                while curNum + 1 in st:
                    curNum += 1
                    l += 1
                res = max(res,l)
        return res
