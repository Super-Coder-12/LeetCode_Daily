def integerBreak(n: int) -> int:
    memo = {}

    def dfs(n):
        if n in memo:
            return memo[n]
        if n == 2:
            return 1
        max_product = 0
        for i in range(1, n):
            max_product = max(max_product, i * max(n - i, dfs(n - i))) ##### 分治
        memo[n] = max_product
        return max_product

    return dfs(n)

# 示例测试
print(integerBreak(2))  # 输出: 1
print(integerBreak(10))  # 输出: 36
