def firstUniqueChar(s): # "leetcode"
    # create a dict to store all letters
    dict = {}
    for char in s:
        if char in dict:
            dict[char] += 1
        else:
            dict[char] = 1
    
    for idx, value in enumerate(s):
        if dict[value] == 1:
            return idx

    return -1

print(firstUniqueChar("leetcode")) # 输出: 0
print(firstUniqueChar("loveleetcode"))  # 输出: 2
print(firstUniqueChar("aabb"))  # 输出: -1
