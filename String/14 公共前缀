def longestCommonPrefix(self, strs: List[str]) -> str:
    if not strs:
        return ""

    # 从最长的开始往前减
    prefix = strs[0]

    # 遍历数组中的其他字符串
    for s in strs:
        while s[:len(prefix)] != prefix: #但凡发现不符合就去掉最后一个
            prefix = prefix[:-1]
           # if not prefix: ### 注意减没了就马上输出空string
           #     return ""
    
    return prefix
