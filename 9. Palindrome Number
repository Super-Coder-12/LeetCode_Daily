########## easy pythonic way ###########
########## [::-1] ############

class Solution:
    def isPalindrome(self, x: int) -> bool:
        return str(x)[::-1] == str(x)

### Solution是一个类，sol是一个实例，调用这个函数 #
sol = Solution()
print(sol.isPalindrome(121)


#### 
def isPalindrome(x):
    return str(x)[::-1] == str(x)

print(isPalindrome(121))  
