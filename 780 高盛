def reachingPoints(sx, sy, tx, ty):
    while tx > 0 and ty > 0:
        if sx == tx and sy == ty:
            return True
        if tx > ty:
            if ty > sy:
                tx %= ty
            else:
                return (tx - sx) % ty == 0 and (tx - sx) >= 0
        else:
            if tx > sx:
                ty %= tx
            else:
                return (ty - sy) % tx == 0 and (ty - sy) >= 0
    return False
