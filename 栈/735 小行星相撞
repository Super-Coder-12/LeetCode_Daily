class Solution:
    def asteroidCollision(self, asteroids: List[int]) -> List[int]:
        stack = []

        for a in asteroids:
            if a < 0: #遇到负的
                while stack and stack[-1] > 0 and abs(stack[-1]) < abs(a): # 负的多
                    stack.pop()
                if stack and stack[-1] > 0 and abs(stack[-1]) == abs(a):
                    stack.pop()
                    continue
                if (not stack) or stack[-1] < 0:
                    stack.append(a)
                elif stack[-1] > 0:
                    continue
            else:
                stack.append(a)
        return stack

