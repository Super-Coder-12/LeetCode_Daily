class Solution:
    def isValid(self, s: str):
        mapping = {'(':')','[':']','{':'}'}
        stack = []
        
        for char in s:
            if stack and stack[-1] in mapping and mapping[stack[-1]] == char:
                stack.pop()
            else:
                stack.append(char)
        return stack == []

