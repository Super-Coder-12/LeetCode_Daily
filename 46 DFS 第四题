def permute(self, nums: List[int]) -> List[List[int]]:
    def dfs(start):
        if start == len(nums):
            results.append(nums[:])
            return
        for i in range(start, len(nums)):
            nums[start], nums[i] = nums[i], nums[start]  ## 交换
            dfs(start + 1)
            nums[start], nums[i] = nums[i], nums[start]

    results = []
    dfs(0)
    return results
