####### 杨辉三角 ################
def generate(numRows):
    triangle = [] 

    for row_num in range(numRows):
        # The first and last row elements are always 1.
        this_row = [None for element in range(row_num + 1)] ## 我觉得None可以用0或1或任何
        this_row[0] = 1
        this_row[-1] = 1

        # Each triangle element (except the first and last of each row)
        # is equal to the sum of the elements above-and-to-the-left and above-and-to-the-right.
        for j in range(1, len(this_row) - 1):
            this_row[j] = triangle[row_num - 1][j - 1] + triangle[row_num - 1][j]

        triangle.append(this_row)

    return triangle

# 测试代码
print(generate(5))
