################# Fight for Goldman! ####################

def compress(self, chars: List[str]) -> int:
########## 读写指针 ########## 读写的部分都在while循环内部
    i,j = 0,0

    while i < len(chars):
        char = chars[i] ###### 要写的char
        count = 0

##### 读的部分
        while i < len(chars) and chars[i] == char:   ##### count直到最后一个相同的char
            i += 1
            count += 1

##### 写的部分，写char本身和数字
        chars[j] = char
        j += 1
        
        if count > 1:
            for digit in str(count): ##### 数字转化为string
                chars[j] = digit
                j += 1
    return j ##### 返回的是长度
