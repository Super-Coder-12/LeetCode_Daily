def lengthOfLongestSubstring(s: str) -> int:
    seen = set()
    left = 0
    max_length = 0

    for i in range(len(s)):
        while s[right] in seen: ####### right见过时候，反而动左边
            seen.remove(s[left]) ######## 记住 set操作，remove和add
            left += 1 
        seen.add(s[i])
        max_length = max(max_length, i - left + 1)

    return max_length

# set的用法
