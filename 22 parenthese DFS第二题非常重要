def generateParenthesis(n):
    def backtrack(s, left, right): //leftAddedSoFar, rightAddedSoFar
        // base case
        if len(s) == 2 * n:
            res.append(s)
            return
        // 两个分支，左边加‘（’，右边加‘）’
        if left < n:
            backtrack(s + '(', left + 1, right)
        if right < left:
            backtrack(s + ')', left, right + 1)
    
    res = []
    backtrack('', 0, 0)
    return res

# 示例用法:
n = 3
print(generateParenthesis(n))

TC: O(2^2N)
SC: O(N)
