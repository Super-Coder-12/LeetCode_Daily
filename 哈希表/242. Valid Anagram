def isAnagram(s: str, t: str) -> bool:
    if len(s) != len(t):
        return False

    count = [0] * 26  # 假设都是小写字母
    for i in range(len(s)):
        count[ord(s[i]) - ord('a')] += 1
        count[ord(t[i]) - ord('a')] -= 1
    
    return all(c == 0 for c in count)


# 示例测试
print(isAnagram("anagram", "nagaram"))  # 输出: True
print(isAnagram("rat", "car"))  # 输出: False
